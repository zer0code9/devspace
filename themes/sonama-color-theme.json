// the terminal
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightBlack": "#000000",
		"terminal.ansiWhite": "#FFFFFF",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBlue": "#FF00FF",
		"terminal.ansiBrightBlue": "#FF00FF",
		"terminal.ansiCyan": "#EE82EE",
		"terminal.ansiBrightCyan": "#EE82EE",
		"terminal.ansiGreen": "#FFD700",
		"terminal.ansiBrightGreen": "#FFD700",
		"terminal.ansiMagenta": "#FFA500",
		"terminal.ansiBrightMagenta": "#FFA500",
		"terminal.ansiRed": "#9A0000",
		"terminal.ansiBrightRed": "#9A0000",
		"terminal.ansiYellow": "#BB2222",
		"terminal.ansiBrightYellow": "#BB2222",
		"terminalCursor.background": "#000000",
		"terminalCursor.foreground": "#FF0000",

		"textLink.activeForeground": "#FF0000",
		"textLink.foreground": "#9a009a",

		"tree.indentGuidesStroke": "#0f0f0f",
		"widget.shadow": "#000000",
	  },

	  "tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#FFA500",
				"fontStyle": ""
			}
		},
		{
		  "name": "Variables Other",
		  "scope": [
			"variable.other.readwrite.js",
			"variable.other.object.js",
			"variable.parameter.js"
		  ],
		  "settings": {
			"foreground": "#FF0000"
		  }
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type.js",
				"storage.type.function.python",
				"constant.language",
				"keyword.control",
				"keyword.control.conditional.js"
			],
			"settings": {
				"foreground": "#FFA500",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Functions/Methods",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.class",
				"entity.name",
				"support.type.python"
			],
			"settings": {
				"foreground": "#FF0000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#FF8C00",
				"fontStyle": ""
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#FFA500",
				"fontStyle": ""
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#FF8C00",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation & Operators",
			"scope": [
				"keyword.operator.assignment.js",
				"keyword.operator.assignment.python",
				"punctuation",
				"punctuation.definition.comment",
				"meta.brace.square.js",
				"meta.brace.round.js",
				"punctuation.definition",
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
				"punctuation.definition.string.begin.python",
				"punctuation.definition.string.end.python",
				"punctuation.separator.comma.js",
				"keyword.operator.relational.js"
			],
			"settings": {
				"foreground": "#9A0000",
				"fontStyle": ""
			}
		},
		{
		  "scope": [
			"markup.inserted"
		  ],
		  "settings": {
			"foreground": "#FFD700"
		  }
		},
		{
		  "scope": [
			"markup.deleted"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"markup.changed"
		  ],
		  "settings": {
			"foreground": "#9A0000"
		  }
		},
		{
		  "scope": [
			"string.regexp"
		  ],
		  "settings": {
			"foreground": "#FFD700"
		  }
		},
		{
		  "scope": [
			"constant.character.escape"
		  ],
		  "settings": {
			"foreground": "#FFD700"
		  }
		},
		{
		  "name": "Data",
		  "scope": [
			"*url*",
			"*link*",
			"*uri*"
		  ],
		  "settings": {
			"fontStyle": "underline"
		  }
		},
		{
		  "scope": [
			"source.js constant.other.object.key.js string.unquoted.label.js"
		  ],
		  "settings": {
			"foreground": "#FF0000",
			"fontStyle": "italic"
		  }
		},
		{
		  "scope": [
			"support.type.property-name.json"
		  ],
		  "settings": {
			"foreground": "#FF0000"
		  }
		},
		{
		  "scope": [
			"text.html.markdown",
			"punctuation.definition.list_item.markdown"
		  ],
		  "settings": {
			"foreground": "#FF0000"
		  }
		},
		{
		  "scope": [
			"text.html.markdown markup.inline.raw.markdown"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"text.html.markdown markup.inline.raw.markdown",
			"punctuation.definition.raw.markdown"
		  ],
		  "settings": {
			"foreground": "#8A2BE2"
		  }
		},
		{
		  "scope": [
			"text.html.markdown meta.dummy.line-break"
		  ],
		  "settings": {}
		},
		{
		  "scope": [
			"markdown.heading",
			"markup.heading | markup.heading entity.name",
			"markup.heading.markdown punctuation.definition.heading.markdown",
			"entity.name.section.markdown"
		  ],
		  "settings": {
			"foreground": "#FFA500",
			"fontStyle": "bold"
		  }
		},
		{
		  "scope": [
			"markup.underline"
		  ],
		  "settings": {
			"foreground": "#FF00FF",
			"fontStyle": "underline"
		  }
		},
		{
		  "scope": [
			"markup.strike"
		  ],
		  "settings": {
			"fontStyle": ""
		  }
		},
		{
		  "scope": [
			"markup.quote punctuation.definition.blockquote.markdown"
		  ],
		  "settings": {
			"foreground": "#8A2BE2"
		  }
		},
		{
		  "scope": [
			"markup.quote"
		  ],
		  "settings": {
			"fontStyle": "italic"
		  }
		},
		{
		  "scope": [
			"string.other.link.description.title.markdown"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"constant.other.reference.link.markdown"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"markup.raw.block"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"punctuation.definition.raw.markdown",
			"punctuation.definition.markdown"
		  ],
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": [
			"variable.language.fenced.markdown"
		  ],
		  "settings": {
			"foreground": "#9a009a"
		  }
		},
		{
		  "scope": [
			"meta.separator"
		  ],
		  "settings": {
			"foreground": "#A7A8AF",
			"fontStyle": "bold"
		  }
		},
		{
		  "scope": "token.info-token",
		  "settings": {
			"foreground": "#9a009a"
		  }
		},
		{
		  "scope": "token.warn-token",
		  "settings": {
			"foreground": "#FFA500"
		  }
		},
		{
		  "scope": "token.error-token",
		  "settings": {
			"foreground": "#FF0000"
		  }
		},
		{
		  "scope": "token.debug-token",
		  "settings": {
			"foreground": "#FFA500"
		  }
		}
	  ]
  }