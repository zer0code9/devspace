		"tab.unfocusedActiveForeground": "#9A0000",
		"tab.unfocusedActiveBackground": "#000000",
		"tab.unfocusedActiveBorder": "#9A009A",
		"tab.unfocusedHoverForeground": "#9A0000",
		"tab.unfocusedHoverBackground": "#000000",
		"tab.unfocusedHoverBorder": "#9A009A",
		"tab.unfocusedInactiveForeground": "#FF9A9A",
		"tab.unfocusedInactiveBackground": "#000000",
// breadbar (top)
		"breadcrumb.activeSelectionForeground": "#FF0000",
		"breadcrumb.background": "#000000",
		"breadcrumb.focusForeground": "#FF0000",
		"breadcrumb.foreground": "#9A0000",
		"breadcrumbPicker.background": "#000000",
// sideBar (left)
		"sideBar.background": "#000000",
		"sideBar.border": "#9a009a",
		"sideBar.foreground": "#FF0000",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.border": "#000000",
		"sideBarSectionHeader.foreground": "#FF00FF",
		"sideBarTitle.foreground": "#FF0000",
// activityBar (far-left)
		"activityBar.foreground": "#FF0000",	
		"activityBar.background": "#000000",
		"activityBar.border": "#9A009A",
		"activityBar.inactiveForeground": "#9A0000",
		"activityBarBadge.foreground": "#000000",
		"activityBarBadge.background": "#9A00FF",
		"activityBar.activeBackground": "#000000",
// mapBar (left)
		"minimap.background": "#000000",
		"minimapGutter.addedBackground": "#FFD700",
		"minimapGutter.deletedBackground": "#FFA500",
		"minimapGutter.modifiedBackground": "#9A0000",
		"minimapSlider.activeBackground": "#9A00007f",
		"minimapSlider.background": "#9A00007f",
		"minimapSlider.hoverBackground": "#BB22227f",
// scrollBar (far-left)
		"scrollbar.shadow": "#000000",
		"scrollbarSlider.activeBackground": "#9a009a7f",
		"scrollbarSlider.background": "#9a009a7f",
		"scrollbarSlider.hoverBackground": "#8A2BE27f",
