		"notificationLink.foreground": "#B529FE",
		"notifications.background": "#000000",
		"notifications.foreground": "#FF1A0D",

		"panel.background": "#000000",
		"panel.border": "#000000",
		"panelTitle.activeBorder": "#B529FE",
		"panelTitle.activeForeground": "#FF1A0D",
		"panelTitle.inactiveForeground": "#990404",
// The box when chaging a file
		"peekView.border": "#000000",
		"peekViewEditor.background": "#1a1a1a",
		"peekViewEditor.matchHighlightBackground": "#B529FE7f",
		"peekViewEditorGutter.background": "#FF1A0D05",
		"peekViewResult.background": "#FF1A0D05",
		"peekViewResult.matchHighlightBackground": "#a80ffa",
		"peekViewResult.selectionBackground": "#a80ffa90",
		"peekViewTitle.background": "#1a1a1a7f",
		"peekViewTitleDescription.foreground": "#FF1A0D",

		"pickerGroup.foreground": "#B529FE",
		"progressBar.background": "#B529FE",

		"selection.background": "#a80ffa",
// the settings
		"settings.checkboxBackground": "#000000",
		"settings.checkboxForeground": "#FF1A0D",
		"settings.dropdownBackground": "#000000",
		"settings.dropdownForeground": "#FF1A0D",
		"settings.headerForeground": "#FF1A0D",
		"settings.modifiedItemIndicator": "#B529FE",
		"settings.numberInputBackground": "#000000",
		"settings.numberInputForeground": "#FF1A0D",
		"settings.textInputBackground": "#000000",
		"settings.textInputForeground": "#FF1A0D",
// bar at the left
		"activityBar.background": "#000000",
		"activityBar.border": "#000000",
		"activityBar.foreground": "#FF1A0D",
		"activityBarBadge.background": "#b91212",
		"activityBarBadge.foreground": "#000000",

		"badge.background": "#000000",
		"badge.foreground": "#B529FE",
// the map (right editor)
		"minimap.background": "#000000",
		"minimapGutter.addedBackground": "#FFD827",
		"minimapGutter.deletedBackground": "#FF8D00",
		"minimapGutter.modifiedBackground": "#990404",
		"minimapSlider.activeBackground": "#860b047f",
		"minimapSlider.background": "#860b047f",
		"minimapSlider.hoverBackground": "#b912127f",
// scroolbar
		"scrollbar.shadow": "#000000",
		"scrollbarSlider.activeBackground": "#B529FE7f",
		"scrollbarSlider.background": "#B529FE7f",
		"scrollbarSlider.hoverBackground": "#a80ffa7f",
// bar with folders and files
		"sideBar.background": "#000000",
		"sideBar.border": "#B529FE",
		"sideBar.foreground": "#FF1A0D",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.border": "#000000",
		"sideBarSectionHeader.foreground": "#E93BFF",
		"sideBarTitle.foreground": "#FF1A0D",
// bar at the bottom
		"statusBar.background": "#000000",
		"statusBar.border": "#000000",
		"statusBar.foreground": "#FF1A0D",
		"statusBar.debuggingBackground": "#000000",
		"statusBar.debuggingForeground": "#860b04",
		"statusBar.noFolderBackground": "#000000",
		"statusBarItem.hoverBackground": "#FF1A0D7f",
		"statusBarItem.remoteBackground": "#B529FE",
		"statusBarItem.remoteForeground": "#000000",
// the tabs (above editor)
		"tab.activeModifiedBorder": "#B529FE",
		"tab.border": "#000000",
		"tab.activeBackground": "#a80ffa7f",
		"tab.activeBorder": "#B529FE",
		"tab.activeForeground": "#b91212",
		"tab.inactiveBackground": "#000000",
		"tab.inactiveForeground": "#b91212",
		"tab.unfocusedActiveBackground": "#B529FE40",
		"tab.unfocusedActiveBorder": "#B529FE",
		"tab.unfocusedActiveForeground": "#860b04",
		"tab.unfocusedInactiveBackground": "#000000",
		"tab.unfocusedInactiveForeground": "#860b04",
// the terminal
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#E93BFF",
		"terminal.ansiBrightBlack": "#B529FE",
		"terminal.ansiBrightBlue": "#E93BFF",
		"terminal.ansiBrightCyan": "#FFD25A",
		"terminal.ansiBrightGreen": "#990404",
		"terminal.ansiBrightMagenta": "#FF8D00",
		"terminal.ansiBrightRed": "#FFD827",
		"terminal.ansiBrightWhite": "#FF6C6C",
		"terminal.ansiBrightYellow": "#FF8D00",
		"terminal.ansiCyan": "#FFD25A",
		"terminal.ansiGreen": "#990404",
		"terminal.ansiMagenta": "#FF8D00",
		"terminal.ansiRed": "#FFD827",
		"terminal.ansiWhite": "#FF6C6C",
		"terminal.ansiYellow": "#FF8D00",
		"terminalCursor.background": "#000000",
		"terminalCursor.foreground": "#FF1A0D",

		"textLink.activeForeground": "#FF1A0D",
		"textLink.foreground": "#B529FE",

		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#FF1A0D",
		"titleBar.border": "#000000",
		"titleBar.inactiveBackground": "#000000",
		"titleBar.inactiveForeground": "#990404",

		"tree.indentGuidesStroke": "#0f0f0f",
		"widget.shadow": "#000000",
	  },
	  "tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#FF8D00",
				"fontStyle": ""
			}
		},
		{
		  "name": "Variables Other",
		  "scope": [
			"variable.other.readwrite.js",
			"variable.other.object.js",
			"variable.parameter.js"
		  ],
		  "settings": {
			"foreground": "#FF1A0D"
		  }
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type.js",
				"storage.type.function.python",
				"constant.language",
				"keyword.control",
				"keyword.control.conditional.js"
			],
			"settings": {
				"foreground": "#FF8D00",
				"fontStyle": ""
			}
		},
		{
			"name": "Functions/Methods",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.class",
				"entity.name",
				"support.type.python"
			],
			"settings": {
				"foreground": "#FF1A0D",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#f06509",
				"fontStyle": ""
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#FF8D00",
				"fontStyle": ""
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f06509",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation & Operators",
			"scope": [
				"keyword.operator.assignment.js",
				"keyword.operator.assignment.python",
				"punctuation",
				"punctuation.definition.comment",
				"meta.brace.square.js",
				"meta.brace.round.js",
				"punctuation.definition",
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
				"punctuation.definition.string.begin.python",
				"punctuation.definition.string.end.python",
				"punctuation.separator.comma.js",
				"keyword.operator.relational.js"
			],
			"settings": {
				"foreground": "#860b04",
				"fontStyle": ""
			}
		},
		{
		  "scope": [
			"markup.inserted"
		  ],
		  "settings": {
			"foreground": "#FFD827"
		  }
		},
		{
		  "scope": [
			"markup.deleted"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"markup.changed"
		  ],
		  "settings": {
			"foreground": "#990404"
		  }
		},
		{
		  "scope": [
			"string.regexp"
		  ],
		  "settings": {
			"foreground": "#FFD25A"
		  }
		},
		{
		  "scope": [
			"constant.character.escape"
		  ],
		  "settings": {
			"foreground": "#FFD25A"
		  }
		},
		{
		  "scope": [
			"*url*",
			"*link*",
			"*uri*"
		  ],
		  "settings": {
			"fontStyle": "underline"
		  }
		},
		{
		  "scope": [
			"source.js constant.other.object.key.js string.unquoted.label.js"
		  ],
		  "settings": {
			"foreground": "#FF1A0D",
			"fontStyle": "italic"
		  }
		},
		{
		  "scope": [
			"support.type.property-name.json"
		  ],
		  "settings": {
			"foreground": "#FF1A0D"
		  }
		},
		{
		  "scope": [
			"text.html.markdown",
			"punctuation.definition.list_item.markdown"
		  ],
		  "settings": {
			"foreground": "#FF1A0D"
		  }
		},
		{
		  "scope": [
			"text.html.markdown markup.inline.raw.markdown"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"text.html.markdown markup.inline.raw.markdown",
			"punctuation.definition.raw.markdown"
		  ],
		  "settings": {
			"foreground": "#a80ffa"
		  }
		},
		{
		  "scope": [
			"text.html.markdown meta.dummy.line-break"
		  ],
		  "settings": {}
		},
		{
		  "scope": [
			"markdown.heading",
			"markup.heading | markup.heading entity.name",
			"markup.heading.markdown punctuation.definition.heading.markdown",
			"entity.name.section.markdown"
		  ],
		  "settings": {
			"foreground": "#FF8D00",
			"fontStyle": "bold"
		  }
		},
		{
		  "scope": [
			"markup.underline"
		  ],
		  "settings": {
			"foreground": "#E93BFF",
			"fontStyle": "underline"
		  }
		},
		{
		  "scope": [
			"markup.strike"
		  ],
		  "settings": {
			"fontStyle": ""
		  }
		},
		{
		  "scope": [
			"markup.quote punctuation.definition.blockquote.markdown"
		  ],
		  "settings": {
			"foreground": "#a80ffa"
		  }
		},
		{
		  "scope": [
			"markup.quote"
		  ],
		  "settings": {
			"fontStyle": "italic"
		  }
		},
		{
		  "scope": [
			"string.other.link.description.title.markdown"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"constant.other.reference.link.markdown"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"markup.raw.block"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"punctuation.definition.raw.markdown",
			"punctuation.definition.markdown"
		  ],
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": [
			"variable.language.fenced.markdown"
		  ],
		  "settings": {
			"foreground": "#B529FE"
		  }
		},
		{
		  "scope": [
			"meta.separator"
		  ],
		  "settings": {
			"foreground": "#A7A8AF",
			"fontStyle": "bold"
		  }
		},
		{
		  "scope": "token.info-token",
		  "settings": {
			"foreground": "#B529FE"
		  }
		},
		{
		  "scope": "token.warn-token",
		  "settings": {
			"foreground": "#FF8D00"
		  }
		},
		{
		  "scope": "token.error-token",
		  "settings": {
			"foreground": "#FF1A0D"
		  }
		},
		{
		  "scope": "token.debug-token",
		  "settings": {
			"foreground": "#FF8D00"
		  }
		}
	  ]
  }